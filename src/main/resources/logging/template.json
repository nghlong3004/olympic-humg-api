{
  "instant": {
    "$resolver": "timestamp",
    "pattern": {
      "format": "yyyy-MM-dd'T'HH:mm:ss.SSS'Z'",
      "timeZone": "UTC"
    }
  },
  "level": {
    "$resolver": "level",
    "field": "severity",
    "severity": {
      "field": "code"
    }
  },
  "message": {
    "$resolver": "message",
    "stringified": false
  }
}